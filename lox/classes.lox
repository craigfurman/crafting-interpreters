class Bagel {
  eat() {
    print "yum!";
  }

  whatIsAdded() {
    return this.added;
  }
}

var bagel = Bagel();

print Bagel;
print bagel;

bagel.added = "capers";
print bagel.added;

fun add(ingredient) {
  print "added " + ingredient;
}
bagel.add = add;
bagel.add("more capers");

bagel.eat();

print bagel.whatIsAdded();
